      "source": [
        "list1 = [25, 63, 10, 87, 41, 99, 34, 56, 70, 51, 12]\n",
        "product = 1\n",
        "\n",
        "for i in list1:\n",
        "  if i == 0:\n",
        "    product = 0\n",
        "    break\n",
        "  else:\n",
        "    product *= i\n",
        "\n",
        "print('Product of list is:',product)\n"
